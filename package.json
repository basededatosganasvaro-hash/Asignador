{
  "name": "sistema-asignacion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && npx tsx prisma/migrate-usernames.ts && npx prisma db push --schema=prisma/schema.prisma --accept-data-loss && npx prisma db seed && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "npm run prisma:generate",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma && prisma generate --schema=prisma/schema-clientes.prisma && prisma generate --schema=prisma/schema-capacidades.prisma",
    "prisma:push": "prisma db push --schema=prisma/schema.prisma",
    "prisma:push-clientes": "prisma db push --schema=prisma/schema-clientes.prisma",
    "seed:embudo": "npx tsx prisma/seed-embudo.ts",
    "migrate:usernames": "npx tsx prisma/migrate-usernames.ts",
    "prisma:index-clientes": "npx tsx prisma/add-indexes-clientes.ts"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/x-data-grid": "^8.27.0",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.97.0",
    "bcryptjs": "^3.0.3",
    "exceljs": "^4.4.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "openai": "^6.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^5.22.0",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
